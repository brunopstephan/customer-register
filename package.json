{
  "name": "aws-project",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "sam local start-api",
    "build": "yarn test && tsup src && sam build",
    "build-start": "yarn build && yarn start",
    "deploy": "sam deploy",
    "deploy-guided": "sam deploy --guided",
    "lint": "eslint --ext .ts src --fix",
    "test": "jest"
  },
  "files": [
    "/dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.731.1",
    "@aws-sdk/util-dynamodb": "^3.731.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/aws-lambda": "^8.10.147",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/test/",
      "/public/",
      "/coverage/"
    ],
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/test/",
      "/public/",
      "/coverage/"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
